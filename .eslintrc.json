{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-var": "error",
    "one-var": ["error", "never"],
    "new-cap": "error",
    "new-parens": "error",
    "constructor-super": "error",
    "eqeqeq": "error",
    "prefer-const": [
      "error",
      {
        "destructuring": "all",
        "ignoreReadBeforeAssign": false
      }
    ]
  }
}
